
Import-Package: \
	javax.sql*, \
	!*

Export-Package: \
	org.apache.commons.dbcp*;version="1.2.2", \
	javax.persistence.*;version="1.1.0", \
	org.hibernate*;version=${project.version};-split-package:=merge-last, \
	net.sf.cglib*;version="2.1_3", \
	javax.annotation*;version="1.0.0"
	
Embed-Dependency: !javax.sql*, \
	*

